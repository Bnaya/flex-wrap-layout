{
  "name": "flex-wrap-layout",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/flex-wrap-layout.git"
  },
  "description": "Experiments with flexbox layout",
  "license": "MIT",
  "keywords": [
    "flexbox",
    "layout",
    "grid"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist build yarn-error.log",
    "clean:all": "yarn clean && rm -rf node_modules yarn.lock",
    "build:umd": "NODE_ENV=development rollup --config && NODE_ENV=production rollup --config",
    "build:esnext": "tsc --noEmit false --outDir dist --module esnext --declaration true",
    "build:cjs": "tsc --noEmit false --outDir dist/cjs",
    "build:css": "node-sass --output-style expanded --source-map true --source-map-contents true src/flex-wrap-layout.scss dist/flex-wrap-layout.css",
    "build": "yarn build:umd && yarn build:esnext && yarn build:cjs && yarn build:css",
    "build:examples": "webpack -d",
    "build:examples:watch": "webpack -d --watch",
    "tsc": "tsc",
    "prettier": "prettier --write \"**/*\"",
    "eslint": "eslint . '**/*.{js,jsx,ts,tsx}'",
    "prepublishOnly": "yarn clean && yarn tsc && yarn eslint && yarn build",
    "publish:beta": "yarn publish --tag next --no-git-tag-version --no-push # Custom example: 0.1.0-beta.1"
  },
  "devDependencies": {
    "@types/node": "^11.11.6",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/webpack": "^4.4.26",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "bootstrap": "^4.3.1",
    "bootstrap-sass": "^3.4.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "rollup": "^1.7.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
